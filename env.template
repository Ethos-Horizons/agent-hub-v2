# Agent Hub Environment Variables
# Copy this file to .env.local and fill in your values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# AI Model Configuration (for agent creation and execution)
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# JWT Secret (generate a random string)
JWT_SECRET=your_jwt_secret

# Encryption Key (generate a random string)
ENCRYPTION_KEY=your_encryption_key

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:5000

# Optional: AI Agent Creation Settings
# AI_CREATION_MODEL=gpt-4-turbo
# AI_CODING_MODEL=claude-3.5-sonnet
# AI_RESEARCH_MODEL=gpt-4-turbo
